#!/bin/bash
# Find what SSNs have been used for account creation then find all eligibility calls with that SSN.

LOGS=/web/stage.texes-ets.org/public/logs/web0[1234]ets56*_le_stage*
ONDATE='11 Mar 2016'

USEDSSNS=$( grep "$ONDATE" $LOGS | grep -o 'SocialSecurityNumber>[^<]\+<' | grep -o '[0-9]\+' )

for SSN in $USEDSSNS
do
	echo -e "**************\n$SSN\n**************\n"
	grep "$ONDATE" $LOGS | grep $SSN
done
